{
  "name": "realnsleo/slim-starter",
  "description": "A Slim framework starter skeleton with PHP-Di integration and Docker Compose.",
  "keywords": [
    "slim",
    "psr7",
    "php-di",
    "framework"
  ],
  "homepage": "http://github.com/realnsleo/slim-skeleton",
  "license": "MIT",
  "authors": [
    {
      "name": "Arnold Babasa",
      "email": "realnsleo@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "slim/slim": "^3.9",
    "monolog/monolog": "^1.17",
    "nesbot/carbon": "^1.24",
    "slim/twig-view": "^2.4",
    "php-di/slim-bridge": "^2.0",
    "hassankhan/config": "^2.0",
    "catfan/medoo": "^1.6",
    "container-interop/container-interop": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.8 < 6.0",
    "vlucas/phpdotenv": "^2.5",
    "symfony/var-dumper": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "config": {
    "process-timeout": 0
  }
}
